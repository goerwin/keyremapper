{
  "description": ["KeyPresses"],
  "keyPressesDelay": 50,
  "keyPresses": [
    { "key": "Caps", "ifPressedNTimes": 1, "send": "Esc" },
    { "key": "Alt", "ifPressedNTimes": 1, "send": "Esc:down Esc:up" },
    { "key": "Win", "ifPressedNTimes": 2, "send": "Shift" },
    { "key": "R", "ifPressedNTimes": 1, "send": "A" }
  ],
  "keybindings": [
    {
      "keys": ["A"],
      "send": ["B:down", "B:up"]
    },
    {
      "keys": ["Win", "Alt"],
      "send": [null, null]
    }
  ],
  "tests": [
    ["A Caps", "B Caps Esc"],
    ["A:down Caps:down A:up Caps:up", "B:down Caps:down B:up Caps:up"],
    ["Win", ""],
    ["Win Win C", "Shift C"],
    ["Win C:down Win", "C:down"],
    ["Caps", "Caps Esc"],
    ["Shift:down Caps Shift:up", "Shift:down Caps Esc Shift:up"],
    ["Caps:down Win:down Caps:up Win:up", "Caps"],

    ["R", "R A"],

    // WITHOUT/WITH DELAY
    ["Alt:down Alt:up", "Esc"],
    ["Alt:down delay:10 Alt:up", "Esc"],
    ["Alt:down delay:51 Alt:up", ""],
    ["Alt:down delay:10 Alt:down Alt:up", "Esc"],
    ["Alt:down delay:51 Alt:down Alt:up", ""],
    ["Alt:down delay:51 Alt:down Alt:up Alt:down delay:10 Alt:up", "Esc"],
    ["A Caps:down Caps:up", "B Caps Esc"],
    ["A Caps:down delay:10 Caps:up", "B Caps Esc"],
    ["A Caps:down delay:51 Caps:up", "B Caps"],
    ["Shift:down Win Shift:up", "Shift:down Shift:up"],
    ["A:down Alt:down delay:10 Alt:up A:up", "B:down Esc B:up"],
    ["A:down Alt:down delay:51 Alt:up A:up", "B:down B:up"]
  ]
}
