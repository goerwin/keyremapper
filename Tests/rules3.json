{
  "description": ["currentKey", "set", "setOnKeyUp", "when", "isKeyDown"],
  "keybindings": [
    {
      "keys": ["A"],
      "fire": ["B currentKey", null]
    },
    {
      "keys": ["C"],
      "set": { "RANDOM_STR": true },
      "setOnKeyUp": { "RANDOM_STR": false }
    },
    {
      "keys": ["C"],
      "when": { "C": false },
      "set": { "RANDOM_STR": false }
    },
    {
      "keys": ["D"],
      "when": { "RANDOM_STR": true },
      "fire": ["X", null]
    },
    {
      "keys": ["D"],
      "when": { "RANDOM_STR": false },
      "fire": ["Y", null]
    },
    {
      "keys": ["E"],
      "when": { "isKeyDown": true },
      "fire": ["A", "B"]
    },
    {
      "keys": ["E"],
      "when": { "isKeyDown": false },
      "fire": ["C", "D"]
    }
  ],
  "tests": [
    ["A", "B A"],
    ["C:down D C:up", "C:down X C:up"],
    ["C D C:down D C:up", "C Y C:down X C:up"],
    ["E:down", "A"],
    ["E:down E:up", "A D"]
  ]
}
